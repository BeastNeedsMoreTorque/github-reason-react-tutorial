ninja_required_version = 1.5.1 
bs_package_flags = -bs-package-name bs-jest
src_root_dir = /Users/jfriend/code/github-re/node_modules/bs-jest
bsc = /Users/jfriend/code/github-re/node_modules/bs-platform/bin/bsc.exe
bsdep = /Users/jfriend/code/github-re/node_modules/bs-platform/bin/bsb_helper.exe
ocamllex = ocamllex.opt
bsc_flags = -nostdlib -I '/Users/jfriend/code/github-re/node_modules/bs-platform/lib/ocaml' -no-alias-deps -color always -w -40+6+7+27+32..39+44+45
ppx_flags = 
bs_package_includes = 
bs_package_dev_includes = 
refmt = /Users/jfriend/code/github-re/node_modules/bs-platform/bin/refmt.exe
reason_react_jsx = 
refmt_flags = --print binary
bsb_dir_group = 0
bsc_lib_includes = -I src
bsc_group_1_includes = 
bsc_group_2_includes = 
bsc_group_3_includes = 
bsc_group_4_includes = 
rule build_ast_and_deps
  command = ${bsc}  ${pp_flags} ${ppx_flags} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/jest.mliast : build_ast_and_deps $src_root_dir/src/jest.mli
rule build_deps
  command = ${bsdep} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/jest.mliast.d : build_deps src/jest.mliast
rule build_cmi
  command = ${bsc} ${bs_package_flags} -bs-no-builtin-ppx-mli -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${bsc_flags} -o ${out} -c  ${in}
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/jest.cmi : build_cmi src/jest.mliast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build src/jest.mlast : build_ast_and_deps $src_root_dir/src/jest.ml
build src/jest.mlast.d : build_deps src/jest.mlast
rule build_cmj_only
  command = ${bsc} ${bs_package_flags} -bs-assume-has-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include  ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${bsc_flags} -o ${in} -c  ${in} ${postbuild}
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/jest.cmj $src_root_dir/lib/js/src/jest.js : build_cmj_only src/jest.mlast |  src/jest.cmi
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build build.ninja : phony  ||  src/jest.mlast.d src/jest.mliast.d
